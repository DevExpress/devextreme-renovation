{
  "name": "@devextreme-generator/inferno-from-react",
  "version": "3.1.0-next",
  "description": "DevExtreme inferno from react component generator",
  "author": "Developer Express Inc.",
  "license": "UNLICENSED",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "build:watch": "tsc --project ./tsconfig.build.json --watch",
    "pack": "npm run build && npm pack && shx cp *.tgz package.tgz",
    "clear": "shx rm -rf dist && shx rm -f *.tgz",
    "update:testcases": "tsc --project ./tsconfig.react.json"
  },
  "dependencies": {
    "@devextreme-generator/core": "3.1.0-next"
  },
  "devDependencies": {
    "@devextreme-generator/build-helpers": "3.1.0-next",
    "@types/node": "^12.12.14",
    "shx": "^0.3.3",
    "typescript": "4.3.5",
    "@devextreme-generator/build-helpers": "3.1.0-next"
  }
}
