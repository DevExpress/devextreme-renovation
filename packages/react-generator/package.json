{
  "name": "@devextreme-generator/react",
  "version": "3.1.0-next",
  "description": "DevExtreme react component generator",
  "author": "Developer Express Inc.",
  "license": "UNLICENSED",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "build:watch": "tsc --project ./tsconfig.build.json --watch",
    "pack": "npm run build && npm pack && npx shx cp *.tgz package.tgz",
    "clear": "npx shx rm -rf dist && npx shx rm -f *.tgz"
  },
  "dependencies": {
    "@devextreme-generator/core": "3.1.0-next"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "shx": "^0.3.3",
    "typescript": "3.8"
  }
}
