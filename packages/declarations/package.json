{
  "name": "@devextreme-generator/declarations",
  "version": "3.1.0-next",
  "description": "DevExtreme declarations",
  "author": "Developer Express Inc.",
  "license": "UNLICENSED",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "build:watch": "tsc --project ./tsconfig.build.json --watch",
    "pack": "npm run build && npm pack && npx shx cp *.tgz package.tgz",
    "clear": "npx shx rm -rf dist && npx shx rm -f *.tgz"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "shx": "^0.3.3",
    "typescript": "3.8"
  }
}
