{
  "name": "@devextreme-generator/preact",
  "version": "3.1.0-next",
  "description": "DevExtreme preact component generator",
  "author": "Developer Express Inc.",
  "license": "UNLICENSED",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "build:watch": "tsc --project ./tsconfig.build.json --watch",
    "pack": "npm run build && npm pack && shx cp *.tgz package.tgz",
    "clear": "shx rm -rf dist && shx rm -f *.tgz"
  },
  "dependencies": {
    "@devextreme-generator/core": "3.1.0-next",
    "@devextreme-generator/react": "3.1.0-next"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "shx": "^0.3.3"
  }
}
