{
  "name": "@devextreme-generator/build-helpers",
  "version": "3.1.0-next",
  "description": "DevExtreme development utils",
  "author": "Developer Express Inc.",
  "license": "UNLICENSED",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "build:watch": "tsc --project ./tsconfig.build.json --watch",
    "pack": "npm run build && npm pack && npx shx cp *.tgz package.tgz",
    "clear": "npx shx rm -rf dist && npx shx rm -f *.tgz"
  },
  "dependencies": {
    "@devextreme-generator/angular": "3.1.0-next",
    "@devextreme-generator/core": "3.1.0-next",
    "@devextreme-generator/inferno": "3.1.0-next",
    "@devextreme-generator/preact": "3.1.0-next",
    "@devextreme-generator/react": "3.1.0-next",
    "loader-utils": "^2.0.0",
    "typescript": "3.8",
    "vinyl": "^2.2.1",
    "webpack": "^5.64.1"
  },
  "devDependencies": {
    "@types/loader-utils": "^2.0.1",
    "@types/vinyl": "^2.0.4",
    "shx": "^0.3.3"
  }
}
