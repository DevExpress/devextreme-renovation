{
  "name": "@devextreme/vdom",
  "version": "1.0.1",
  "description": "DevExtreme virtual DOM common components",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
