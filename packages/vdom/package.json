    "inferno": "^7.4.6",
    "inferno-compat": "^7.4.6",
    "inferno-create-element": "^7.4.6",
    "inferno-hydrate": "^7.4.6"
  },
  "devDependencies": {
    "shx": "^0.3.3",
    "typescript": "3.8"
  }
}
