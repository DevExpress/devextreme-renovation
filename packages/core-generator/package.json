{
  "name": "@devextreme-generator/core",
  "version": "2.2.1",
  "description": "DevExtreme core generator",
  "author": "Developer Express Inc.",
  "license": "UNLICENSED",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "build:watch": "tsc --project ./tsconfig.build.json --watch",
    "pack": "npm run build && npm pack && shx cp *.tgz package.tgz",
    "clear": "shx rm -rf dist && shx rm -f *.tgz"
  },
  "dependencies": {
    "code-block-writer": "^10.1.0",
    "prettier": "^2.1.0",
    "typescript": "3.8"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "@types/prettier": "^2.1.0",
    "shx": "^0.3.3"
  }
}
